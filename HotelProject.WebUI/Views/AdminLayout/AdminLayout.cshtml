
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">


@await Html.PartialAsync("/Views/AdminLayout/HeadPartial.cshtml")

<body>

    @await Html.PartialAsync("/Views/AdminLayout/PreloaderPartial.cshtml")

    <!--**********************************
        Main wrapper start
    ***********************************-->
    <div id="main-wrapper">

        @await Html.PartialAsync("/Views/AdminLayout/NavHeaderPartial.cshtml")

        @await Html.PartialAsync("/Views/AdminLayout/HeaderPartial.cshtml")


        <partial name="~/Views/AdminLayout/SidebarPartial.cshtml" />

        <div class="content-body">


            <div class="container-fluid">
                <div class="row">
                    <div class="col-12">
                        <div class="card">
                            <div class="card-body">
                                <p>@RenderBody()</p>
                                <br />
                               
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <partial name="~/Views/AdminLayout/FooterPartial.cshtml" />
    </div>
    <partial name="~/Views/AdminLayout/ScriptPartial.cshtml" />

</body>

</html>
